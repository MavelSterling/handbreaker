{
  "version": 2,
  "builds": [
    {
      "src": "breaker_game.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/breaker_game.html"
    }
  ],
  "outputDirectory": ".",
  "public": true
} 